        SUBROUTINE CLIN2C       This subroutine produces patients to order by size and ageC		and permits entry of pulmonary function tests to make a model.C		J. Michael Dean   December 12, 1994C		(Finally after 15 years!)        DIMENSION T(120),LARRAY(3,10)	    COMMON KT,KL,INI,NW1,NW2,JKL,IFLAG,NEOF,NW        COMMON NFLAG,J3,ISPAR,NA,NB,NC,ND,NE(8)        COMMON NARTI,MT,K2,K4,INDEX        COMMON FIO2,FIC2,CO,PD,FADM,BULLA,VLUNG,ELAST,VADM,AZ        COMMON BZ,CZ,BARPR,TEMP,TRQ,TC2MT,TVOL,HB,PCV,VBLVL        COMMON ADDC3,BC3AJ,DPG,PR,FITNS,SPACE,COMAX,SHUNT,VC,PEEP        COMMON VO2CT,PA,RPH,VPH,FVENT,BPH,BAGO,BAGC,AO2MT,AC2MT        COMMON AO2PR,AC2PR,DPH,XLACT,BO2PR,BC2PR,TIDVL,RRATE,RO2CT,VC2MT        COMMON DVENT,SVENT,PC2CT,PO2CT,TO2CT,TC2CT,BO2CT,BC2CT,TPH,RC3CT        COMMON VC2CT,RO2MT,RC2MT,XRESP,AN2MT,BO2MT,BC2MT,CBF,PC,DSPAC        COMMON REFLV,RO2PR,CONSO,RC2PR,PG,PJ,TND,RC2CT,QB,PW        COMMON FT,CONOM,BUBBL,TC2RF,TC3MT,VC3MT,TC3CT,VC3CT,TLAMT,RLACT        COMMON BC3CT,BO2AD,COADJ,EO2CT,TO2MT,TO2PR,TC2PR,VO2MT,AVENT,PL        COMMON EC2CT,TN2MT,TN2PR,FEV,SN2PR,EN2CT,UN2MT,RN2MT,X109,X110        COMMON TC3AJ,SN2MT,QA,RVADM,XDSPA,BAG,XMALE,HT,WT,AGE	    EQUIVALENCE (FIO2,T(1))CC		LARRAY stores normal function test results for functionsC		used later in FUNC1 and FUNC2		DATA LARRAY/.026,.009,2.18,.047,.0075,4.583,	  /	.0452,.024,2.852,.0582,.025,4.241,.035,.025,1.932,	  /	.0363,.032,1.26,.382,.732,100.,-.125,.951,100.,	  /	.221,.683,100.,-.011,.782,100./		FUNC1(HT,AGE,X,Y,Z)=X*HT-Y*AGE-Z		FUNC2(HT,X,Y)=X+(HT*.01)**3*YC		Initialize predicted FEV, VC, and DCO in case next section gets skipped		PFEV=FEV		PVC=VC		PDCO=20.		IF (NW2.GT.19) GO TO 110		WRITE(*,*) '70 Kg man - Do you want someone else?  1. Yes 2. No'		NW2=0	110	CALL NXTWD(7,XXX,NNN,1,2)		GO TO (120,280), NNNC		Build up the specified subject	120	IF (NW2.GT.19) GO TO 140		WRITE(*,*) 'Please specify 1. Male 2. Female'		NW2=0	140	CALL NXTWD(7,XXX,NNN,1,2)		IF (NNN.EQ.2) XMALE=0.		IF (NW2.GT.19) GO TO 160		WRITE(*,*) 'Give me the height in CM (95 to 200)'		NW2=0	160	CALL NXTWD(7,HT,NNN,95,200)		IF (NW2.GT.19) GO TO 180		WRITE(*,*) 'Now the weight in kilograms (20 to 200)'		NW2=0	180	CALL NXTWD(7,WT,NN,20,200)		RAT = WT*5880./HT**1.6		IF (XMALE.LT..5) RAT = RAT*1.064		IF (NW2.GT.19) GO TO 200		WRITE(*,*) 'And the age in years (8 to 100)'		NW2=0	200	CALL NXTWD(7,AGE,NNN,8,100)		K=INT(XMALE)+1		VLUNG=FUNC1(HT,AGE,LARRAY(1,K),LARRAY(2,K),LARRAY(3,K))		K=K+2		X=ABS(20.-AGE)		PDCO=(7.6*VLUNG+5.)*(100.-X)*.01		VLUNG=VLUNG*1000.		IF(AGE.LT.17.) GO TO 212		PVC=FUNC1(HT,AGE,LARRAY(1,K),LARRAY(2,K),LARRAY(3,K))		K=K+2		PFEV=FUNC1(HT,AGE,LARRAY(1,K),LARRAY(2,K), LARRAY(3,K))		GO TO 220	212	K=K+4		PVC=FUNC2(HT,LARRAY(1,K),LARRAY(2,K))		K=K+2		PFEV=FUNC2(HT,LARRAY(1,K),LARRAY(2,K))	220	CONSO=WT**.75*10.33		X=PVC-PFEV-.1		IF (X.LT.0) PVC = PFEV+.1		CONOM=CONSO*0.0195		IF (XMALE.GT..5) GO TO 230		CONOM = CONOM*.9		HB=13.5		PCV=41.	230	VBLVL=CONOM*300.+1500.		TVOL=(WT**.6*.465)+6.		X=TVOL*.084		TC3MT=TC3MT*X**1.35		TC2MT=TC2MT*X		SN2MT=SN2MT*X				